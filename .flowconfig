[ignore]
.*/node_modules/babel.*
.*/node_modules/editions/.*
.*/node_modules/fbjs/.*
.*/node_modules/immutable/.*
.*/node_modules/.*.json
.*/redux-crud-view/node_modules/.*

[include]

[libs]
./interfaces/modules.js.flow
./interfaces/index.js.flow
./flow-typed/npm/axios_v0.11.x.js
./flow-typed/npm/debug_v2.x.x.js
./flow-typed/npm/mocha_v2.4.x.js
./flow-typed/npm/redux-actions_v0.9.x.js
./node_modules/immutable/dist/immutable.js.flow

[options]
esproposal.class_instance_fields=enable
esproposal.class_static_fields=enable
module.system=haste
unsafe.enable_getters_and_setters=true
